{
  extends: ['config:base'],
  dependencyDashboard: true,
  ignoreDeps: [],
  rebaseWhen: 'behind-base-branch',
  branchConcurrentLimit: 10,
  lockFileMaintenance: {
    enabled: true,
    automerge: true,
  },
  addLabels: ['renovate-update'],
  cdnurl: {
    fileMatch: ['^assets/.*\\.tsx?$'],
  },
  packageRules: [
    {
      matchUpdateTypes: ['minor', 'patch'],
      matchCurrentVersion: '!/^0/',
      automerge: true,
    },
  ],
}
